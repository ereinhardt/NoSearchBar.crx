{
  "author": {
    "name": "Erik Anton Reinhardt",
    "url": "https://ereinhardt.net"
  },
  "manifest_version": 2,
  "name": "NoSearchBar",
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "NoSearchBar",
      "keyword": "*",
      "suggest_url": "https://*.net",
      "search_url": "https://*.net",
      "favicon_url": "https://*.net/favicon.ico",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "Remove SearchBars from the Internet.",
  "version": "1.2",
  "icons": {
    "48": "./images/icon.png"
  },
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": [
      "./script.js",
      "opencv.js"
    ],
    "run_at": "document_start"
  }],
  "web_accessible_resources": ["template.png"]
}
